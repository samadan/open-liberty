-privatepackage: suite.oracle.suite3

# In a multiple suite FAT bucket, this is required to say which suite this bundle contains
suite.name=suite.oracle.suite3.FATSuite

# Define additional tested features that are NOT present in any XML files in this bucket.
# In this case, servlet-4.0 is added programmatically at runtime by the RepeatTests rule.
tested.features:\
  databaseRotation,\
  cdi-2.0, cdi-3.0, cdi-4.0,\
  el-3.0,\
  jdbc-4.2,\
  jndi-1.0,\
  jsp-2.3,\
  servlet-3.1, servlet-4.0, servlet-5.0, servlet-6.0
