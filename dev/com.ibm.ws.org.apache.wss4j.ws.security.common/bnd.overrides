-include= ~../cnf/resources/bnd/rejar.props
bVersion=2.3.0

WS-TraceGroup: WSS4J

Export-Package: \
 org.apache.ws.security;version=1.6.2, \
 org.apache.ws.security.components.crypto;version=1.6.2, \
 org.apache.wss4j.common.*;version=${bVersion}
  
# these should be optional only when resolving jars from wlp/dev
Import-Package: \
 !com.ibm.security.jgss, \
 !com.sun.security.jgss, \
 !org.bouncycastle.asn1, \
 !org.bouncycastle.asn1.x500, \
 !org.bouncycastle.asn1.x509, \
 com.ibm.websphere.ras, \
 com.ibm.websphere.ras.annotation, \
 com.ibm.ws.ffdc, \
 org.apache.xml.security.*;version="[1.0.0,5.0.0)",\
 org.apache.neethi.*;version="[3.0.0,4.0.0)",\
 org.apache.wss4j.*;version="[2.3.0,5.0.0)", \
 org.apache.ws.security.*;version="[1.0.0,5.0.0)", \
 org.ehcache.core.*;version="[3.0.0,4.0.0)",\
 *
 
DynamicImport-Package: \
 org.bouncycastle.jce.provider,\
 org.apache.wss4j.common.crypto,\
 org.apache.wss4j.common.saml
 
 

