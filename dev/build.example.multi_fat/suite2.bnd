-privatepackage: com.ibm.ws.example.suite2

# Suite name is required to be able to designate specific properties for this suite bundle
suite.name=com.ibm.ws.example.suite2.FATSuite

# EE 10 repeat is the lowest repeat done in this suite, so need the minimum Java to be Java 11
fat.minimum.java.level: 11

# Define additional tested features that are NOT present in any XML files in this bucket.
# In this case, servlet-6.0 is added programmatically at runtime by the RepeatTests rule.
tested.features:\
	servlet-6.0,\
	servlet-6.1
