-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

# Modeled off com.ibm.ws.jaxrs.2.x.config
Bundle-Name: Channel Framework Common Configuration
Bundle-SymbolicName: io.openliberty.channel.config.common
Bundle-Description: IBM Channel Framework Common Configuration; version=${bVersion}

WS-TraceGroup: ChannelFramework

IBM-Default-Config: OSGI-INF/wlp/defaultInstances.xml

Export-Package: io.openliberty.channel.config;version=${bVersion}

Private-Package: io.openliberty.channel.config.internal

Include-Resource: \
 OSGI-INF=resources/OSGI-INF

-dsannotations: \
 	io.openliberty.channel.config.internal.ChannelFrameworkConfigImpl

 -buildpath: \
 	com.ibm.ws.logging,\
    com.ibm.websphere.org.osgi.core,\
	com.ibm.websphere.org.osgi.service.component,\
	com.ibm.wsspi.org.osgi.service.component.annotations,\
    com.ibm.ws.kernel.service;version=latest