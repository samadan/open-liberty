<infinispan>
  <jgroups>
     <stack-file name="jgroups-tcp" path="/default-configs/default-jgroups-tcp.xml"/>
  </jgroups>
  
  <cache-container>
    <transport cluster="${infinispan.cluster.name}" stack="jgroups-tcp"/>
    <replicated-cache-configuration name="com.ibm.ws.session.attr.*"/>
    <replicated-cache-configuration name="com.ibm.ws.session.meta.*"/>
  </cache-container>
</infinispan>
